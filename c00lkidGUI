local function QEUNGS_fake_script() -- fly.LocalScript 
	local script = Instance.new('LocalScript', fly)

	local flyEvent = script.Parent.Parent.Parent:WaitForChild("FlyEvent")
	local button = script.Parent -- O botão no GUI
	local player = game.Players.LocalPlayer
	local userInputService = game:GetService("UserInputService")
	local camera = game.Workspace.CurrentCamera
	
	button.MouseButton1Click:Connect(function()
		local isFlying = player:GetAttribute("IsFlying") -- Checa se já está voando
	
		-- Envia a requisição para o servidor para ativar/desativar o fly
		flyEvent:FireServer(not isFlying) -- Passa "true" ou "false" para ativar/desativar
	end)
	
	-- Lógica para controlar o movimento durante o voo
	userInputService.InputBegan:Connect(function(input, gameProcessedEvent)
		if player:GetAttribute("IsFlying") then
			local character = player.Character
			local humanoidRootPart = character and character:FindFirstChild("HumanoidRootPart")
	
			if humanoidRootPart then
				local bodyVelocity = humanoidRootPart:FindFirstChild("BodyVelocity")
				local bodyGyro = humanoidRootPart:FindFirstChild("BodyGyro")
	
				if bodyVelocity and bodyGyro then
					local moveDirection = Vector3.new(0, 0, 0)
	
					-- Teclas WASD ou flechas para movimento horizontal
					if input.KeyCode == Enum.KeyCode.W then
						moveDirection = camera.CFrame.LookVector
					elseif input.KeyCode == Enum.KeyCode.S then
						moveDirection = -camera.CFrame.LookVector
					elseif input.KeyCode == Enum.KeyCode.A then
						moveDirection = -camera.CFrame.RightVector
					elseif input.KeyCode == Enum.KeyCode.D then
						moveDirection = camera.CFrame.RightVector
						-- Tecla E para ir para cima
					elseif input.KeyCode == Enum.KeyCode.E then
						moveDirection = Vector3.new(0, 1, 0) -- Direção para cima
						-- Tecla Q para ir para baixo
					elseif input.KeyCode == Enum.KeyCode.Q then
						moveDirection = Vector3.new(0, -1, 0) -- Direção para baixo
					end
	
					-- Aplique a velocidade na direção desejada
					bodyVelocity.Velocity = moveDirection * 50 -- Ajuste a velocidade de voo como necessário
	
					-- Mantém a altura fixa, para que o personagem "flutue" um pouco para cima
					local currentPos = humanoidRootPart.Position
					humanoidRootPart.CFrame = CFrame.new(currentPos.X, currentPos.Y + 0.5, currentPos.Z)
	
					-- Mantém o personagem alinhado com a câmera (direção do olhar)
					bodyGyro.CFrame = camera.CFrame
				end
			end
		end
	end)
	
	-- Lógica para parar o voo quando o jogador soltar as teclas
	userInputService.InputEnded:Connect(function(input)
		if player:GetAttribute("IsFlying") then
			if input.KeyCode == Enum.KeyCode.W or input.KeyCode == Enum.KeyCode.S or input.KeyCode == Enum.KeyCode.A or input.KeyCode == Enum.KeyCode.D or input.KeyCode == Enum.KeyCode.E or input.KeyCode == Enum.KeyCode.Q then
				-- Se o jogador não estiver pressionando nenhuma tecla de movimento, pare o movimento
				local character = player.Character
				local humanoidRootPart = character and character:FindFirstChild("HumanoidRootPart")
				local bodyVelocity = humanoidRootPart:FindFirstChild("BodyVelocity")
				if bodyVelocity then
					bodyVelocity.Velocity = Vector3.new(0, 0, 0) -- Não permite movimento quando as teclas são soltas
				end
			end
		end
	end)
	
end
coroutine.wrap(QEUNGS_fake_script)()
local function XLAO_fake_script() -- NEXT_2.LocalScript 
	local script = Instance.new('LocalScript', NEXT_2)

	local btn = script.Parent
	local frm = script.Parent.Parent
	local frm2 = script.Parent.Parent.Parent.Frame2
	
	btn.MouseButton1Click:Connect(function()
		frm2.Visible = true
		frm.Visible = false
	end)
end
coroutine.wrap(XLAO_fake_script)()
