		local username = "c00lkids095" -- O nome do personagem que você quer virar
	
		-- Envia o evento para o servidor com o nome do usuário
		ChangeCharacterEvent:FireServer(username)
	end)
	
end
coroutine.wrap(AVNOBZ_fake_script)()
local function WMYNHO_fake_script() -- TextBox.LocalScript 
	local script = Instance.new('LocalScript', TextBox)

	local changeSpeedEvent = script.Parent.Parent.Parent:WaitForChild("ChangeSpeedEvent")
	local textBox = script.Parent -- ou qualquer que seja o seu TextBox
	
	textBox.FocusLost:Connect(function(enterPressed)
		if enterPressed then
			local speed = tonumber(textBox.Text)
			if speed then
				changeSpeedEvent:FireServer(speed) -- Envia o novo speed para o servidor
			end
		end
	end)
	
end
coroutine.wrap(WMYNHO_fake_script)()
