coroutine.wrap(RZJNWMX_fake_script)()
local function WZVVYN_fake_script() -- c00lparts.LocalScript 
	local script = Instance.new('LocalScript', c00lparts)

	local button = script.Parent -- o botão que vai fazer as duas funções
	local imgId = "http://www.roblox.com/asset/?id=1365169976" -- id do decal
	local decalsAdded = false -- estado que controla se os decals estão aplicados ou não
	
	button.MouseButton1Click:Connect(function()
		if decalsAdded then
			-- se os decals estão aplicados, remove eles
			for _, part in pairs(workspace:GetDescendants()) do
				if part:IsA("BasePart") then
					-- remove o decal da parte
					for _, decal in pairs(part:GetChildren()) do
						if decal:IsA("Decal") then
							decal:Destroy()
						end
					end
				end
			end
			decalsAdded = false -- atualiza o estado para 'não aplicados'
		else
			-- se os decals não estão aplicados, adiciona eles
			for _, part in pairs(workspace:GetDescendants()) do
				if part:IsA("BasePart") then
					-- cria e aplica o decal
					local decal = Instance.new("Decal")
					decal.Texture = imgId
					decal.Parent = part
				end
			end
			decalsAdded = true -- atualiza o estado para 'aplicados'
		end
	end)
	
end
coroutine.wrap(WZVVYN_fake_script)()
local function PXKRG_fake_script() -- night.LocalScript 
	local script = Instance.new('LocalScript', night)

	-- dentro do botão de noite (por exemplo: NightButton)
	local nightOn = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if nightOn then
			game.Lighting.ClockTime = 14 -- volta pra tarde
			nightOn = false
		else
			game.Lighting.ClockTime = 0 -- ativa noite
			nightOn = true
		end
	end)
	
end
coroutine.wrap(PXKRG_fake_script)()
