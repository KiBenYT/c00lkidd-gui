			workspaceSound:Play()
		end
	end)
	
end
coroutine.wrap(PVJYHDL_fake_script)()
local function CUUAKHY_fake_script() -- c00lmessage.LocalScript 
	local script = Instance.new('LocalScript', c00lmessage)

	local mensagens = {
		"IM TELLING DAD!",
		"HAHAHAHA..."
	}
	
	local sons = {
		["IM TELLING DAD!"] = "rbxassetid://82347871291422",
		["HAHAHAHA..."] = "rbxassetid://126083075694948"
	}
	
	script.Parent.MouseButton1Click:Connect(function()
		local jogador = game.Players.LocalPlayer
		local personagem = jogador.Character or jogador.CharacterAdded:Wait()
		local head = personagem:WaitForChild("Head")
	
		local msg = mensagens[math.random(1, #mensagens)]
	
		local existente = head:FindFirstChild("MensagemGui")
		if existente then
			existente:Destroy()
		end
	
		local billboard = Instance.new("BillboardGui")
		billboard.Name = "MensagemGui"
		billboard.Adornee = head
		billboard.Size = UDim2.new(0, 200, 0, 50)
		billboard.StudsOffset = Vector3.new(0, 2.5, 0)
		billboard.AlwaysOnTop = true
		billboard.Parent = head
	
		local texto = Instance.new("TextLabel")
		texto.Size = UDim2.new(1, 0, 1, 0)
		texto.BackgroundTransparency = 1
		texto.TextScaled = true
		texto.TextColor3 = Color3.new(1, 0, 0)
		texto.Text = msg
		texto.Font = Enum.Font.FredokaOne
		texto.Parent = billboard
	
		if sons[msg] then
			local sound = Instance.new("Sound")
			sound.SoundId = sons[msg]
			sound.Volume = 1
			sound.Parent = personagem:FindFirstChild("HumanoidRootPart") or head
			sound:Play()
			game.Debris:AddItem(sound, 10)
		end
	
		task.delay(2, function()
			if billboard and billboard.Parent then
				billboard:Destroy()
			end
		end)
	end)
	
end
coroutine.wrap(CUUAKHY_fake_script)()
