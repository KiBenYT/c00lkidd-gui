-- Scripts:

local function AKQXJ_fake_script() -- ScreenGui.redpartschanger 
	local script = Instance.new('LocalScript', ScreenGui)

	local redPartsEvent = script.Parent.RedPartsEvent
	
	redPartsEvent.OnServerEvent:Connect(function(player)
		-- Iterar por todas as partes no Workspace e mudar a cor para vermelho
		for _, object in pairs(workspace:GetDescendants()) do
			if object:IsA("BasePart") then
				object.BrickColor = BrickColor.new("Bright red")  -- Troca a cor para vermelho
			end
		end
	end)
	
end
coroutine.wrap(AKQXJ_fake_script)()
local function LLOMVKX_fake_script() -- ScreenGui.skyboxchanger 
	local script = Instance.new('Script', ScreenGui)

	local skyboxChangeEvent = script.Parent.SkyboxChangeEvent -- Acessa o RemoteEvent dentro do ScreenGui
	
	skyboxChangeEvent.OnServerEvent:Connect(function(player)
		print("Evento de mudança de Skybox recebido pelo servidor!") -- Mensagem de depuração
	
		-- Verificando se a skybox já existe
		if game.Lighting:FindFirstChild("CustomSkybox") then
			print("A Skybox já foi aplicada.") -- Se já existir, não vai criar outra
		else
			local skybox = Instance.new("Sky") -- Cria a Skybox
			skybox.Name = "CustomSkybox" -- Nomeia a Skybox
			skybox.Parent = game.Lighting -- Coloca no Lighting (aonde a Skybox é aplicada)
	
			-- Definindo os IDs dos decais da Skybox
			skybox.SkyboxBk = "http://www.roblox.com/asset/?id=15198649455" -- Fundo
			skybox.SkyboxDn = "http://www.roblox.com/asset/?id=15198649455" -- Abaixo
			skybox.SkyboxFt = "http://www.roblox.com/asset/?id=15198649455" -- Frente
			skybox.SkyboxLf = "http://www.roblox.com/asset/?id=15198649455" -- Esquerda
			skybox.SkyboxRt = "http://www.roblox.com/asset/?id=15198649455" -- Direita
			skybox.SkyboxUp = "http://www.roblox.com/asset/?id=15198649455" -- Cima
	
			print("Skybox foi aplicada com sucesso!") -- Mensagem de sucesso
		end
	end)
	
end
coroutine.wrap(LLOMVKX_fake_script)()
local function QXVXR_fake_script() -- ScreenGui.characterchanger 
	local script = Instance.new('Script', ScreenGui)

	local ChangeCharacterEvent = script.Parent:WaitForChild("ChangeCharacterEvent")
	local Players = game:GetService("Players")
	
	ChangeCharacterEvent.OnServerEvent:Connect(function(player, username)
		local success, userId = pcall(function()
			return Players:GetUserIdFromNameAsync(username)
		end)
	
		if success then
			-- Cria o modelo do personagem com base no UserId
			local charModel = Players:CreateHumanoidModelFromUserId(userId)
			charModel.Name = player.Name
	
			-- Salva a posição do personagem antigo
			local oldChar = player.Character
			local pos = oldChar and oldChar:FindFirstChild("HumanoidRootPart") and oldChar.HumanoidRootPart.CFrame
	
			-- Troca o personagem
			player.Character = charModel
			charModel.Parent = workspace
	
			-- Define a posição do novo personagem
			if pos then
				task.wait()
				local root = charModel:FindFirstChild("HumanoidRootPart")
				if root then
					root.CFrame = pos
				end
			end
	
			-- Apaga o personagem antigo
			if oldChar then
				oldChar:Destroy()
			end
		end
	end)
	
end
coroutine.wrap(QXVXR_fake_script)()
local function ELQMX_fake_script() -- ScreenGui.flyevent 
	local script = Instance.new('Script', ScreenGui)

	local flyEvent = script.Parent:WaitForChild("FlyEvent")
	local Players = game:GetService("Players")
	
	flyEvent.OnServerEvent:Connect(function(player, activate)
		local character = player.Character
		if character then
			local humanoid = character:FindFirstChildOfClass("Humanoid")
			if humanoid then
				-- Se ativar o fly
				if activate then
					local bodyGyro = Instance.new("BodyGyro")
					bodyGyro.MaxTorque = Vector3.new(400000, 400000, 400000)
					bodyGyro.CFrame = character.HumanoidRootPart.CFrame
					bodyGyro.P = 125000
					bodyGyro.Parent = character.HumanoidRootPart
	
					local bodyVelocity = Instance.new("BodyVelocity")
					bodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
					bodyVelocity.Velocity = Vector3.new(0, 0, 0)  -- Começar sem movimento
					bodyVelocity.Parent = character.HumanoidRootPart
	
					-- Atribui a flag de voo
					player:SetAttribute("IsFlying", true)
	
					-- Se desativar o fly
				else
					-- Remove o BodyGyro e BodyVelocity
					local bodyGyro = character.HumanoidRootPart:FindFirstChild("BodyGyro")
					local bodyVelocity = character.HumanoidRootPart:FindFirstChild("BodyVelocity")
	
					if bodyGyro then bodyGyro:Destroy() end
					if bodyVelocity then bodyVelocity:Destroy() end
	
					player:SetAttribute("IsFlying", false)
				end
			end
		end
	end)
	
end
coroutine.wrap(ELQMX_fake_script)()
local function ZJITV_fake_script() -- ScreenGui.speedchanger 
	local script = Instance.new('Script', ScreenGui)

	local changeSpeedEvent = script.Parent.ChangeSpeedEvent
	
	changeSpeedEvent.OnServerEvent:Connect(function(player, speed)
		local char = player.Character
		if char then
			local humanoid = char:WaitForChild("Humanoid")
			humanoid.WalkSpeed = speed
		end
	end)
	
end
coroutine.wrap(ZJITV_fake_script)()
local function RDLM_fake_script() -- ScreenGui.jumpchanger 
	local script = Instance.new('Script', ScreenGui)

	local changeJumpEvent = script.Parent.ChangeJumpEvent
	
	changeJumpEvent.OnServerEvent:Connect(function(player, jumpPower)
		local char = player.Character
		if char then
			local humanoid = char:WaitForChild("Humanoid")
			humanoid.JumpPower = jumpPower -- Modifica o JumpPower do humanoide
		end
	end)
	
end
coroutine.wrap(RDLM_fake_script)()
local function PUITHG_fake_script() -- skybox.LocalScript 
	local script = Instance.new('LocalScript', skybox)

	local button = script.Parent -- Botão dentro da GUI
	local skyboxChangeEvent = script.Parent.Parent.Parent.SkyboxChangeEvent -- Acessa o RemoteEvent dentro do ScreenGui
	
	button.MouseButton1Click:Connect(function()
		-- Quando o botão for clicado, dispara o evento para o servidor
		print("Botão de Skybox clicado!") -- Mensagem de depuração
		skyboxChangeEvent:FireServer() -- Dispara o evento para o servidor
	end)
	
end
coroutine.wrap(PUITHG_fake_script)()
